{
  "name": "init",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --host 0.0.0.0 --port 3000 --config ./client/webpack.config.js",
    "prod": "cross-env NODE_ENV=production webpack --progress --profile --config ./client/webpack.config.js",
    "pm2": "pm2 start ./server/bin/run",
    "start": "nodemon ./server/bin/run --ignore ./client",
    "start:win": "./server/connect-db.bat & npm start",
    "start:mac": "./server/connect-db.sh & npm start",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register -R spec",
    "test:watch": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register -R spec --watch",
    "test:cover": "cross-env NODE_ENV=test istanbul cover _mocha -- --compilers js:babel-core/register"
  },
